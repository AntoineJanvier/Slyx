                <Rectangle2D />
            </viewport>
        </ImageView>
        <Label fx:id="label_name" layoutX="14.0" layoutY="24.0" text="Name">
            <font>
                <Font size="12.0" />
            </font>
        </Label>
      <Button fx:id="button_reject_request" layoutX="215.0" layoutY="19.0" mnemonicParsing="false" text="X" />
