<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.RadialGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane_contact" prefHeight="62.0" prefWidth="250.0" style="-fx-border-radius: 30;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="slyx.controllers.ContactController">
   <children>
      <ImageView fx:id="imageView_contact_icon" fitHeight="60.0" fitWidth="60.0" layoutX="188.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <viewport>
            <Rectangle2D />
         </viewport>
      </ImageView>
      <Label fx:id="label_firstname" layoutX="7.0" layoutY="7.0" text="Firstname">
         <font>
            <Font size="12.0" />
         </font>
      </Label>
      <Label fx:id="label_lastname" layoutX="7.0" layoutY="25.0" text="Lastname">
         <font>
            <Font size="12.0" />
         </font>
      </Label>
      <Rectangle fx:id="rect_connected" fill="DODGERBLUE" height="5.0" layoutY="57.0" smooth="false" stroke="BLACK" strokeType="INSIDE" width="250.0" />
      <Circle fx:id="circle_notifications" layoutX="146.0" layoutY="27.0" radius="6.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0">
         <fill>
            <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </RadialGradient>
         </fill>
      </Circle>
   </children>
</AnchorPane>
